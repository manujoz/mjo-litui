{
	"servers": {
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp"
			]
		},
		"jira": {
			"type": "stdio",
			"command": "docker",
			"args": [
				"run",
				"--rm",
				"-i",
        		"--env-file",
				"${workspaceFolder}/.settings/mcp-atlassian.env",
				"ghcr.io/sooperset/mcp-atlassian:latest"
			]
		},
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/"
		}
	}
}