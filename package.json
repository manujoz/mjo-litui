{
    "name": "mjo-litui",
    "version": "0.2.8",
    "description": "A collection of web components for building user interfaces with LitElement.",
    "type": "module",
    "main": "index.js",
    "types": "index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./index.js"
        },
        "./mjo-*": {
            "types": "./mjo-*.d.ts",
            "import": "./mjo-*.js"
        },
        "./components/*": {
            "types": "./components/*.d.ts",
            "import": "./components/*.js"
        },
        "./controllers/*": {
            "types": "./controllers/*.d.ts",
            "import": "./controllers/*.js"
        },
        "./locales/*": {
            "types": "./locales/*.d.ts",
            "import": "./locales/*.js"
        },
        "./mixins/*": {
            "types": "./mixins/*.d.ts",
            "import": "./mixins/*.js"
        },
        "./theme/*": {
            "types": "./theme/*.d.ts",
            "import": "./theme/*.js"
        },
        "./types/*": {
            "types": "./types/*.d.ts",
            "import": "./types/*.js"
        },
        "./utils/*": {
            "types": "./utils/*.d.ts",
            "import": "./utils/*.js"
        }
    },
    "files": [
        "*.js",
        "*.d.ts",
        "*.d.ts.map",
        "components/",
        "controllers/",
        "locales/",
        "mixins/",
        "theme/",
        "types/",
        "utils/"
    ],
    "keywords": [
        "web-components",
        "lit-element",
        "lit-html",
        "ui",
        "components"
    ],
    "homepage": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/manujoz/mjo-litui"
    },
    "author": "Manu Overa",
    "license": "MIT",
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc -p tsconfig.build.json && ncp src/types dist/types && ncp package.json ./dist/package.json && ncp .npmignore ./dist/.npmignore && ncp README.md ./dist/README.md",
        "build:public": "npm run build && cd dist && npm publish",
        "preview": "vite preview",
        "test": "npm run build && web-test-runner",
        "test:watch": "npm run build && web-test-runner --watch",
        "test:coverage": "npm run build && web-test-runner --coverage",
        "postinstall": "npx playwright install"
    },
    "dependencies": {
        "@lit/context": "^1.1.3",
        "lit": "^3.2.0",
        "mjo-icons": "^2.0.0"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^1.4.0",
        "@commitlint/cli": "^18.4.4",
        "@commitlint/config-conventional": "^18.4.4",
        "@esm-bundle/chai": "^4.3.4-fix.0",
        "@lit-labs/ssr": "^3.3.1",
        "@lit-labs/ssr-client": "^1.1.7",
        "@lit-labs/ssr-dom-shim": "^1.4.0",
        "@lit-labs/testing": "^0.2.7",
        "@open-wc/testing": "^4.0.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "^20.11.5",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "@web/dev-server-esbuild": "^1.0.4",
        "@web/test-runner": "^0.20.2",
        "@web/test-runner-mocha": "^0.9.0",
        "@web/test-runner-playwright": "^0.11.1",
        "csstype": "^3.1.3",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-lit": "^1.11.0",
        "eslint-plugin-lit-a11y": "^4.1.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-wc": "^2.0.4",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "ncp": "^2.0.0",
        "prettier": "^3.2.4",
        "typescript": "^5.2.2",
        "validate-branch-name": "^1.3.0",
        "vite": "^5.0.8",
        "vite-plugin-svgo": "^1.4.0"
    }
}
